{
  "compilerOptions": {
    "noImplicitAny": false,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest"
    ],
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "jsx": "react-native",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "node_modules/*",
        "src/types/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
